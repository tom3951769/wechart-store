.pills-list {
  list-style: none;
  font-size: 26rpx;
}
.pills-list:after {
  content: '';
  display: table;
  clear: both;
}
.pills-list label {
  float: left;
  position: relative;
  display: block;
  padding: 6px 9px;
  margin: 10px 10px 10px 0;
  border: 1px solid #dedede;
  border-radius: 3px;
}

.page-bottom-dialog {
  font-size:32rpx;
  height: 770rpx;
  width: 100%;
  position: fixed;
  bottom:0;
  float: left;
  z-index: 999;
}
.page-bottom-content {
  position:relative;
  height: 100%;
  margin: 0 auto;
  background-color: #fff;
}
.page-dialog-close{
  position: absolute;
  display: block;
  width: 60rpx;
  height: 60rpx;
  color:black;
  text-align: center;
  justify-content: center;
  border-radius: 50%;
  font-size: 60rpx;
  right: 2%;
  z-index: 1000;
}
.dialog-block-item1 {
  margin:0 5% 0 5%;
  height: 200rpx;
  position: relative;
  border-bottom: 1rpx solid #eee;
